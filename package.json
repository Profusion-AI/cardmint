{
  "name": "cardmint",
  "private": true,
  "version": "1.0.0",
  "description": "High-performance card scanning and processing system",
  "main": "dist/index.js",
  "scripts": {
    "dev": "cross-env NODE_ENV=development concurrently -k -n api,dash -c \"auto\" \"tsx watch src/index.ts\" \"vite\"",
    "dev:api": "cross-env NODE_ENV=development tsx watch src/index.ts",
    "dev:dash": "vite",
    "dashboard": "tsx src/dashboard-server.ts",
    "dev:full": "cross-env NODE_ENV=development concurrently -k -n api,dash -c \"auto\" \"tsx watch src/index.ts\" \"vite\"",
    "dev:operator": "concurrently -k -n api,ui -c \"auto\" \"tsx watch src/server/operator-api.ts\" \"vite --config vite.config.operator.ts\"",
    "build:operator": "vite build --config vite.config.operator.ts",
    "preview:operator": "vite preview --config vite.config.operator.ts",
    "prod:build": "npm run build:api && npm run build:dashboard",
    "prod:dev": "npm run prod:build && tsx src/utils/process-manager.ts",
    "prod:start": "cross-env NODE_ENV=production node dist/index.js",
    "prod:clean": "rm -rf dist/ && rm -rf dashboard/dist/",
    "prod:managed": "npm run prod:build && node -r tsx/cjs src/utils/process-manager.ts",
    "preview": "vite preview --port 5173",
    "build": "tsc -p tsconfig.build.json && vite build",
    "build:observability": "vite build -c vite.config.observability.ts",
    "preview:observability": "vite preview -c vite.config.observability.ts",
    "ci:install-offline": "npm ci --omit=optional --prefer-offline --no-audit --no-fund",
    "ci:assert-node20": "bash scripts/ci/assert-node20.sh",
    "ci:obs-smoke": "bash scripts/ci/obs_smoke.sh",
    "ci:obs-artifacts": "node scripts/validate-artifacts.mjs",
    "ci:forbid-online": "bash scripts/ci/forbid-online.sh",
    "ci:scan-bundle": "node scripts/ci/scan-bundle.mjs",
    "ci:bundle-budget": "node scripts/ci/bundle-budget.mjs",
    "ci:verify-assets": "node scripts/verify-assets.mjs",
    "build:api": "tsc -p tsconfig.build.json",
    "build:dashboard": "vite build",
    "start": "node dist/index.js",
    "test": "jest",
    "test:camera": "tsx test-camera-production.ts",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "typecheck": "tsc --noEmit",
    "profile": "node --inspect dist/index.js",
    "camera:setup": "tsx src/utils/camera-setup.ts",
    "camera:build": "cd src/camera && node-gyp rebuild && cd ../.. && g++ -std=c++17 -o CrSDK_v2.00.00_20250805a_Linux64PC/build/sony-cli src/camera/sony-cli-wrapper.cpp -I CrSDK_v2.00.00_20250805a_Linux64PC/app -L CrSDK_v2.00.00_20250805a_Linux64PC/build -lCr_Core -Wl,-rpath,CrSDK_v2.00.00_20250805a_Linux64PC/build",
    "camera:test": "cd CrSDK_v2.00.00_20250805a_Linux64PC/build && ./sony-cli list",
    "camera:capture": "cd CrSDK_v2.00.00_20250805a_Linux64PC/build && echo -e 'capture\\ncapture\\nquit' | ./sony-cli session",
    "debug-pipeline": "tsx src/utils/debug-pipeline.ts",
    "codemod:ports": "jscodeshift -t tools/codemods/ports-injection.ts src/ --parser tsx --ignore-pattern='**/*.test.ts' --ignore-pattern='**/wiring.ts'",
    "arch:lint": "dependency-cruiser src --validate .dependency-cruiser.cjs",
    "arch:graph": "dependency-cruiser src --output-type dot --config .dependency-cruiser.cjs | dot -T svg > docs/architecture-graph.svg",
    "arch:test": "tsx scripts/test-architecture.ts",
    "gamepad:detect": "tsx scripts/linux/gamepad-detect.ts",
    "gamepad:watch": "tsx scripts/linux/gamepad-detect.ts --watch",
    "gamepad:grab": "python3 scripts/linux/gamepad-grab.py",
    "test:controller": "tsx scripts/test-controller-integration.ts",
    "test:port-resilience": "./scripts/test-port-resilience.sh",
    "test:websocket-discovery": "node scripts/test-websocket-discovery.js",
    "test:performance": "./scripts/monitor-performance.sh 300",
    "test:performance:quick": "./scripts/monitor-performance.sh 60",
    "test:infrastructure": "npm run test:port-resilience && npm run test:websocket-discovery",
    "test:controller:smoke": "npm run test:controller && npm run test:infrastructure",
    "test:controller:full": "echo 'Full controller test suite - Phase 4 implementation pending'",
    "e2e:preflight": "./scripts/e2e-preflight.sh",
    "e2e:run": "./scripts/e2e-run.sh",
    "golden:accuracy": "tsx scripts/golden-accuracy.ts",
    "evaluate:golden10": "tsx src/scripts/evaluate-golden-10.ts",
    "setup:local-first": "tsx src/scripts/setup-local-first.ts",
    "test:local-first": "tsx src/scripts/test-local-first.ts",
    "roi:overlay": "./bin/roi-overlay",
    "roi:explain": "./bin/roi-explain",
    "roi:doctor": "./bin/roi-doctor",
    "roi:performance-gate": "./scripts/roi-performance-gate.sh",
    "test:roi": "jest --testPathPattern='roi.*test'",
    "prepare": "husky",
    "obs:server": "node tests/e2e/observability/server.js",
    "obs:dev": "concurrently -k -n obs-ui,obs-srv -c \"auto\" \"vite\" \"node tests/e2e/observability/server.js\""
  },
  "overrides": {
    "@mendable/firecrawl-js": "0.0.36"
  },
  "keywords": [
    "card",
    "scanning",
    "ocr",
    "real-time",
    "processing"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@mendable/firecrawl-js": "0.0.36",
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/auto-instrumentations-node": "^0.54.0",
    "@opentelemetry/exporter-prometheus": "^0.57.0",
    "@opentelemetry/sdk-node": "^0.57.0",
    "@types/better-sqlite3": "^7.6.13",
    "@types/chokidar": "^1.7.5",
    "@types/commander": "^2.12.0",
    "@types/sqlite3": "^3.1.11",
    "@types/uuid": "^10.0.0",
    "@types/formidable": "^3.4.5",
    "axios": "^1.11.0",
    "formidable": "^3.5.1",
    "better-sqlite3": "^12.2.0",
    "blerg": "^0.0.3",
    "bullmq": "^5.57.0",
    "canvas": "^3.2.0",
    "chokidar": "^4.0.3",
    "commander": "^12.1.0",
    "csv-parse": "^6.1.0",
    "csv-parser": "^3.2.0",
    "dotenv": "^16.4.7",
    "image-hash": "^5.3.2",
    "ioredis": "^5.7.0",
    "node-cache": "^5.1.2",
    "pg": "^8.16.3",
    "pg-pool": "^3.7.0",
    "pino": "^9.5.0",
    "pino-pretty": "^13.0.0",
    "sharp": "^0.34.3",
    "sqlite3": "^5.1.7",
    "tesseract.js": "^6.0.1",
    "uuid": "^11.1.0",
    "ws": "^8.18.0",
    "zod": "^4.1.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.34.0",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/express": "^5.0.3",
    "@types/jest": "^29.5.14",
    "@types/jscodeshift": "^17.3.0",
    "@types/node": "^22.12.0",
    "@types/pg": "^8.11.10",
    "@types/ws": "^8.5.14",
    "@typescript-eslint/eslint-plugin": "^8.21.0",
    "@typescript-eslint/parser": "^8.21.0",
    "@vitest/ui": "^3.2.4",
    "ajv": "^8.17.1",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "dependency-cruiser": "^17.0.1",
    "eslint": "^9.18.0",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "express": "^5.1.0",
    "globals": "^16.3.0",
    "husky": "^9.1.6",
    "jest": "^30.0.0-alpha.7",
    "jscodeshift": "^17.3.0",
    "jsdom": "^26.1.0",
    "lint-staged": "^15.2.10",
    "node-addon-api": "^8.5.0",
    "node-gyp": "^11.0.0",
    "shadcn": "^3.2.1",
    "ts-jest": "^29.2.5",
    "tsx": "^4.20.4",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.41.0",
    "vite": "^5.4.0",
    "vitest": "^3.2.4"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,cjs,mjs,json}": [
      "eslint --fix --max-warnings=0"
    ]
  },
  "engines": {
    "node": ">=20",
    "npm": ">=10"
  }
}
